<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="tasksTf">
    <default-activity>refresh</default-activity>
    <view id="tasks">
      <page>/tasks.amx</page>
    </view>
    <method-call id="refresh">
      <method>#{bindings.refresh.execute}</method>
      <outcome>
        <fixed-outcome>refresh</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>refresh</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>refresh</from-outcome>
        <to-activity-id>tasks</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
